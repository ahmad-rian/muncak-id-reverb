REVERB_APP_KEY=your-app-key
REVERB_APP_SECRET=your-app-secret

# Reverb Client Connection (Public-facing)
REVERB_HOST=pusher.muncak.id
REVERB_PORT=443
REVERB_SCHEME=https

# Reverb Server Configuration (Internal)
REVERB_SERVER_HOST=0.0.0.0
REVERB_SERVER_PORT=8080

# Reverb Server (for production with SSL)
REVERB_TLS_CERT=/path/to/cert.pem
REVERB_TLS_KEY=/path/to/key.pem

# Reverb Scaling (optional)
